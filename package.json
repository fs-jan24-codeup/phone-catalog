    "minimist": "^1.2.8",
    "node-sass": "^9.0.0",
    "openai": "^4.38.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-i18next": "^14.1.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
